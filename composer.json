{
    "name": "umanit-pomm/pomm-symfony-bridge",
    "type": "library",
    "description":  "Pomm2 shared elements between Silex and Symfony.",
    "keywords": ["silex", "symfony","pomm", "debug", "profiler"],
    "license":  "MIT",
    "authors":  [ { "name": "Grégoire HUBERT", "email": "hubert.greg@gmail.com" }, { "name": "Jérôme MACIAS", "email": "jerome.macias@gmail.com" } ],
    "require": {
        "php": ">=7.1",
        "umanit-pomm/foundation": "dev-master",
        "symfony/http-foundation": "~4.0|~5.0|^6.3",
        "symfony/http-kernel": "~4.4|~5.0|^6.3",
        "symfony/serializer": "~4.0|~5.0|^6.3",
        "symfony/property-info": "~4.0|~5.0|^6.3"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~2.7"
    },
    "suggest": {
        "umanit-pomm/model-manager": "~2.0",
        "umanit-pomm/cli": "~2.0",
        "symfony/symfony": "~4.4|~5.0|^6.3"
    },
    "autoload": {
        "psr-4": {
            "PommProject\\SymfonyBridge\\": "sources/lib"
        }
    }
}
